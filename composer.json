{
    "name": "amicrud/amicrud",
    "description": "AmiCRUD is a streamlined Laravel package that accelerates the development of CRUD interfaces",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "focuslegon",
            "email": "33376436+focuslegon@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "maatwebsite/excel": "^3.1",
        "barryvdh/laravel-dompdf": "^2.0"
    },
    "autoload": {
        "files": [
            "helpers/general.php"
        ],
        "psr-4": {
            "AmiCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmiCrud\\AmiCrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
